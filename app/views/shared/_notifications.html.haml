- if notice
  .container
    .alert.alert-dismissable.alert-info
      %button.close{ type: 'button', data: { dismiss: 'close' } } &times;
      = notice
- if alert
  .container
    .alert.alert-dismissable.alert-danger
      %button.close{ type: 'button', data: { dismiss: 'close' } } &times;
      = alert
